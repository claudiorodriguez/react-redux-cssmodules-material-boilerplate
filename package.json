{
  "name": "react-redux-cssmodules-material-boilerplate",
  "version": "0.0.0",
  "description": "A working example of a stub app based on React, Redux, CSS Modules, Material UI, Webpack, and Mocha with Enzyme for testing",
  "main": "./src/server/index.js",
  "scripts": {
    "build:webpack": "./node_modules/.bin/cross-env NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "clean": "./node_modules/.bin/gulp clean",
    "watch": "node ./start.js",
    "start": "node ./src/server/index.js",
    "validate": "./node_modules/.bin/gulp validate",
    "test": "./node_modules/.bin/cross-env NODE_ENV=testing ./node_modules/.bin/mocha --compilers js:babel-core/register --require ./test/helpers/css-modules.js 'test/**/*.spec.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/claudiorodriguez/react-redux-cssmodules-material-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "css-modules",
    "webpack",
    "material",
    "boilerplate"
  ],
  "author": "Claudio Rodriguez <cjrodr@yahoo.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/claudiorodriguez/react-redux-cssmodules-material-boilerplate/issues"
  },
  "homepage": "https://github.com/claudiorodriguez/react-redux-cssmodules-material-boilerplate#readme",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "material-ui": "^0.15.2",
    "react": "^15.2.1",
    "react-css-modules": "^3.7.7",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "roboto-fontface": "^0.5.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.5.0",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.1",
    "del": "^2.2.1",
    "enzyme": "^2.4.1",
    "eslint-plugin-react": "^5.2.2",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-sass-lint": "^1.2.0",
    "mocha": "^2.5.3",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.2.1",
    "react-hot-loader": "^1.3.0",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.1"
  }
}
